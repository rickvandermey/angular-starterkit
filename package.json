{
	"name": "RVDM-Angular-Starterkit",
	"version": "7.0.0",
	"homepage": "/",
	"author": "Rick van der Meij",
	"private": false,
	"license": "ISC",
	"scripts": {
		"affected": "nx affected --base=$npm_config_base",
		"affected:apps": "nx affected:apps --base=$npm_config_base",
		"affected:build": "nx affected:build --base=$npm_config_base",
		"affected:build-ci": "nx affected --target=build-ci --base=$npm_config_base",
		"affected:dep-graph": "nx affected:dep-graph --base=$npm_config_base",
		"affected:e2e": "nx serve mockserver & nx affected:e2e --webdriver-update=false --base=$npm_config_base",
		"affected:lint": "nx workspace-lint && nx affected --target=stylelint --base=$npm_config_base && nx affected:lint --base=$npm_config_base",
		"affected:lint-ci": "nx workspace-lint && nx affected --target=stylelint-ci --base=$npm_config_base && nx affected --target=lint-ci --base=$npm_config_base",
		"affected:test": "nx affected:test --watch=false --browsers Chromium --base=$npm_config_base",
		"affected:test-ci": "nx affected:test --code-coverage --watch=false --browsers ChromiumNoSandbox --base=$npm_config_base",
		"certificate:generate:mac": "cd ci && bash tools/ssl-generator/generateCertificateOSX.sh",
		"certificate:copy": "cp -a ./ssl/default/. ./ssl/.",
		"cypress:open": "cypress open",
		"build": "ng build $APP --prod --configuration=$CONF --ngsw-config-path=./ngsw-config.json",
		"e2e": "nx e2e $APP --webdriver-update=false",
		"e2e:no-serve": "npm run e2e -- --devServerTarget=''",
		"e2e:only-tag": "TAG=@only npm run e2e:no-serve --retry=0",
		"dep-graph": "nx dep-graph",
		"format": "nx format:write",
		"format:check": "nx format:check --base=$npm_config_base",
		"format:write": "nx format:write --base=$npm_config_base",
		"lint": "nx workspace-lint && ng stylelint $APP && ng lint $APP",
		"ng": "nx",
		"nx": "nx",
		"postinstall": "npx rimraf node_modules/node-sass && npm run set-base && ngcc",
		"prepare": "husky install",
		"set-base": "node ./tools/nx-resolver/resolveBase.js",
		"start": "ng serve $APP",
		"test": "ng test $APP --code-coverage --browsers Chromium",
		"update": "nx migrate latest",
		"workspace-generator": "nx workspace-generator"
	},
